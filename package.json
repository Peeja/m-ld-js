{
  "name": "@gsvarovsky/m-ld",
  "version": "0.0.5",
  "description": "m-ld native Javascript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "link-local-m-ld-spec": "sudo npm link ../m-ld-spec",
    "clean": "rm -rf dist",
    "build-quick": "tsc",
    "build": "npm run clean && npm run test && tsc",
    "dev": "jest --watch --silent",
    "test": "jest --silent",
    "compliance": "node compliance/test.js",
    "publish-patch": "npm run build && npm version patch -m \"Patch version\" && git push && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gsvarovsky/m-ld-js.git"
  },
  "author": "gsvarovsky@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gsvarovsky/m-ld-js/issues"
  },
  "homepage": "https://github.com/gsvarovsky/m-ld-js#readme",
  "devDependencies": {
    "@gsvarovsky/m-ld-spec": "0.1.2",
    "@types/jest": "^24.9.1",
    "@types/memdown": "^3.0.0",
    "aedes": "^0.41.0",
    "jest": "^24.9.0",
    "jest-mock-extended": "^1.0.8",
    "leveldown": "^5.6.0",
    "memdown": "^5.1.0",
    "restify": "^8.5.1",
    "tmp": "^0.1.0",
    "ts-jest": "^24.3.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/test/"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    }
  },
  "dependencies": {
    "@types/abstract-leveldown": "^5.0.1",
    "@types/async-lock": "^1.1.2",
    "@types/bn.js": "^4.11.6",
    "@types/jsonld": "^1.5.1",
    "@types/rdf-js": "^2.0.12",
    "@types/readable-stream": "^2.3.5",
    "async-lock": "^1.2.2",
    "async-mqtt": "^2.5.0",
    "bn.js": "^5.1.1",
    "jsonld": "^2.0.2",
    "mqtt-pattern": "^1.2.0",
    "quadstore": "^6.0.1",
    "rxjs": "^6.5.5",
    "short-uuid": "^3.1.1"
  }
}
