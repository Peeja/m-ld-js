{
  "name": "@gsvarovsky/m-ld",
  "version": "0.0.4",
  "description": "m-ld native Javascript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "tsc:version": "tsc --version",
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run test && tsc",
    "dev": "jest --watch --silent",
    "test": "jest --silent",
    "publish-patch": "npm run build && npm version patch -m \"Patch version\" && git push && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gsvarovsky/m-ld-js.git"
  },
  "author": "gsvarovsky@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gsvarovsky/m-ld-js/issues"
  },
  "homepage": "https://github.com/gsvarovsky/m-ld-js#readme",
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/memdown": "^3.0.0",
    "jest": "^24.9.0",
    "jest-mock-extended": "^1.0.8",
    "memdown": "^5.1.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/test/"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    }
  },
  "dependencies": {
    "@types/abstract-leveldown": "^5.0.1",
    "@types/async-lock": "^1.1.1",
    "@types/bn.js": "^4.11.5",
    "@types/jsonld": "^1.5.0",
    "@types/rdf-js": "^2.0.5",
    "@types/readable-stream": "^2.3.5",
    "async-lock": "^1.2.2",
    "async-mqtt": "^2.5.0",
    "bn.js": "^5.0.0",
    "jsonld": "^2.0.1",
    "mqtt-pattern": "^1.2.0",
    "quadstore": "^6.0.1",
    "rxjs": "^6.5.4",
    "short-uuid": "^3.1.1"
  }
}
